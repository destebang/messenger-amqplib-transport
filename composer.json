{
    "name": "destebang/messenger-amqplib-transport",
    "description": "Amqplib adapter for Symfony Messenger component",
    "type": "symfony-bundle",
    "require": {
        "symfony/messenger": "^4.3",
        "php-amqplib/php-amqplib": "^2.9",
        "symfony/options-resolver": "^3.4|^4.2"
    },
    "keywords": ["symfony", "messenger", "amqplib"],
    "homepage": "http://symfony.com",
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/yaml": "^3.4|^4.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Esteban",
            "email": "danielestebangonzalez83@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Destebang\\AmqplibMessengerAdapter\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable"
}
